<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/blankFragmentMenu">

    <fragment
        android:id="@+id/blankFragmentStart"
        android:name="com.ggr3ml1n.quizz.BlankFragmentStart"
        android:label="fragment_start"
        tools:layout="@layout/fragment_blank_start" >
        <action
            android:id="@+id/action_blankFragmentStart_to_blankFragmentMenu"
            app:destination="@id/blankFragmentMenu" />
    </fragment>
    <fragment
        android:id="@+id/blankFragmentMenu"
        android:name="com.ggr3ml1n.quizz.BlankFragmentMenu"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_blank_menu" >
        <action
            android:id="@+id/action_blankFragmentMenu_to_blankFragmentQuestion"
            app:destination="@id/blankFragmentQuestion" />
    </fragment>
    <fragment
        android:id="@+id/blankFragmentQuestion"
        android:name="com.ggr3ml1n.quizz.BlankFragmentQuestion"
        android:label="fragment_blank_question"
        tools:layout="@layout/fragment_blank_question" >
        <action
            android:id="@+id/action_blankFragmentQuestion_to_blankFragmentReturn"
            app:destination="@id/blankFragmentReturn" />
    </fragment>
    <fragment
        android:id="@+id/blankFragmentReturn"
        android:name="com.ggr3ml1n.quizz.BlankFragmentReturn"
        android:label="fragment_blank_return"
        tools:layout="@layout/fragment_blank_return" >
        <action
            android:id="@+id/action_blankFragmentReturn_to_blankFragmentMenu"
            app:destination="@id/blankFragmentMenu" />
    </fragment>
</navigation>